{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  "rules": {
    "indent": ["warn", 2],
    "semi": ["error", "always"],
    "quotes": ["error", "double"],
    "linebreak-style": ["error", "windows"],
    "require-jsdoc": "off"
  }
}
